source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '10.0'

target 'themoviedatabase' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  pod 'Alamofire', '~> 4.7.3'
  pod 'AlamofireObjectMapper', '~> 5.1.0'
  pod 'SwiftyJSON'

  pod 'RxSwift',    '~> 4.0'
  pod 'RxCocoa',    '~> 4.0'

end
